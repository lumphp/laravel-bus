{
    "name": "lumphp/laravel-bus",
    "description": "The Illuminate Bus package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/lumphp/laravel-bus/issues",
        "source": "https://github.com/lumphp/laravel-bus"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "lumphp/laravel-collections": "^8.0",
        "lumphp/laravel-contracts": "^8.0",
        "lumphp/laravel-pipeline": "^8.0",
        "lumphp/laravel-support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Bus\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "suggest": {
        "illuminate/queue": "Required to use closures when chaining jobs (^7.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
